{
  "name": "react-native-linkedin",
  "version": "1.0.4",
  "main": "index.js",
  "repository": "https://github.com/xcarpentier/react-native-linkedin.git",
  "author": "Xavier Carpentier <contact@xaviercarpentier.com> (http://xaviercarpentier.com/)",
  "license": "MIT",
  "scripts": {
    "test": "node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "flow": "flow",
    "publish": "yarn version --new-version $NEW_VERSION && git push --follow-tags"
  },
  "dependencies": {
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-airbnb-flow": "^1.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-react-native": "^2.3.2",
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "flow-bin": "^0.49.1",
    "jest": "^20.0.4"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb-flow"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "react-native"
    ],
    "globals": {
      "__DEV__": false,
      "fetch": false,
      "describe": false,
      "it": false,
      "expect": false,
      "jest": false,
      "alert": false,
      "FormData": false
    },
    "rules": {
      "global-require": [
        0
      ],
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "react/jsx-filename-extension": 0,
      "react/forbid-prop-types": 0,
      "no-return-assign": 0,
      "no-alert": 0,
      "react/prefer-stateless-function": 0,
      "arrow-parens": 0,
      "camelcase": 0,
      "no-underscore-dangle": 0,
      "no-confusing-arrow": 0,
      "semi": [
        "error",
        "never"
      ],
      "max-len": [
        2,
        80,
        2
      ]
    }
  }
}
